{
  "manifest_version": 2,
  "name": "Google Meet Extension",
  "version": "1.0",
  "description": "Description of my extension",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["popup.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "gmeet16.png"
    }
  }
}